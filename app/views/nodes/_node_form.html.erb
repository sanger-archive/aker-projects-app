<%= f.text_field :name, autocomplete: 'off', disabled: !write_permission %>
<%= f.text_field :description, disabled: !write_permission %>
<%= f.text_field :cost_code, disabled: !write_permission, autocomplete: 'off' %>
<%= f.hidden_field :parent_id %>
<%= f.hidden_field :id %>
<%= f.text_field :user_writers, label: "Users who can modify this project", data: { behavior: "selectize" }, disabled: !write_permission %>
<%= f.text_field :group_writers, label: "Groups who can modify this project", data: { behavior: "selectize" }, disabled: !write_permission %>
<%= f.text_field :user_spenders, label: "Users who can spend on this project", data: { behavior: "selectize" }, disabled: !write_permission %>
<%= f.text_field :group_spenders, label: "Groups who can spend on this project", data: { behavior: "selectize" }, disabled: !write_permission %>
